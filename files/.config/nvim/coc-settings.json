{
  "suggest.echodocSupport": true,
  "suggest.autoTrigger": "always",
  "suggest.maxCompleteItemCount": 25,
  "signature.enable": true,
  "signature.target": "echo",
  "typescript.suggest.autoImports": true,
  "typescript.suggestionActions.enabled": true,
  "typescript.suggest.completeFunctionCalls": true,
  "diagnostic.errorSign": "⚡️",
  "diagnostic.warningSign": "?",
  "diagnostic.infoSign": "•",
  "diagnostic.virtualText": true,
  "diagnostic.virtualTextCurrentLineOnly": false,
  "diagnostic.enableMessage": "jump",
  "explorer.icon.enableNerdfont": true,
  "explorer.position": "floating",
  "explorer.quitOnOpen": true,
  "explorer.previewAction.onHover": false,
  "explorer.enableFloatinput": false,
  "explorer.floating.width": 52,
  "explorer.floating.height": -5,
  "explorer.floating.contentWidth": -1,
  "explorer.floating.border.enable": false,
  "explorer.floating.border.title": "Explorer",
  "git.enableGutters": true,
  "coc.preferences.formatOnSaveFiletypes": ["javascript", "typescript", "typescriptreact", "json", "javascriptreact", "typescript.tsx"],
  "prettier.printWidth": 120,
  "prettier.trailingComma": "es5",
  "prettier.requireConfig": true,
  "list.extendedSearchMode": true,
  "explorer.file.root.template": "  [icon]  [root]",
  "explorer.sources": [
    {
      "name": "file",
      "expand": true
    }
  ],
  "list.source.grep.command": "rg",
  "list.source.grep.args": ["--smart-case"]
}
