{
  "suggest.echodocSupport": true,
  "suggest.autoTrigger": "always",
  "suggest.maxCompleteItemCount": 25,
  "signature.enable": true,
  "signature.target": "echo",
  "typescript.suggest.autoImports": true,
  "typescript.suggestionActions.enabled": true,
  "typescript.suggest.completeFunctionCalls": true,
  "diagnostic.errorSign": "⚡️",
  "diagnostic.warningSign": "?",
  "diagnostic.infoSign": "•",
  "diagnostic.virtualText": true,
  "diagnostic.virtualTextCurrentLineOnly": false,
  "diagnostic.enableMessage": "jump",
  "explorer.icon.enableNerdfont": true,
  "explorer.position": "right",
  "explorer.quitOnOpen": true,
  "git.enableGutters": true,
  "coc.preferences.formatOnSaveFiletypes": ["javascript", "typescript", "typescriptreact", "json", "javascriptreact", "typescript.tsx"],
  "prettier.printWidth": 120,
  "prettier.trailingComma": "es5",
  "prettier.requireConfig": true,
  "list.extendedSearchMode": true,
  "list.autoResize": true,
  "list.source.grep.command": "rg",
  "list.source.grep.args": ["--smart-case"]
}
